{
	"version": 3,
	"routes": [
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "^\\/_image$",
			"dest": "_image.astro"
		},
		{
			"src": "^\\/$",
			"dest": "index.astro"
		},
		{
			"src": "^\\/character\\/([^/]+?)\\/?$",
			"dest": "character/[id].astro"
		},
		{
			"src": "^\\/character\\/?$",
			"dest": "character.astro"
		},
		{
			"src": "^\\/location\\/?$",
			"dest": "location.astro"
		},
		{
			"src": "^\\/episode\\/?$",
			"dest": "episode.astro"
		},
		{
			"src": "^\\/home\\/?$",
			"dest": "home.astro"
		}
	]
}